generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  email String @unique
  password String
  name String?
  vehicleNumber String? @unique
  dl String @unique
}

model Owner{
  id Int @id @default(autoincrement())
  email String @unique
  password String
  name String?
  license String @unique
  location String

}